{
  "name": "sistemas-pedidos-comprovantes",
  "version": "0.0.1",
  "description": "Uma API em Node usando typescript.",
  "main": "index.js",
  "scripts": {
    "start": "tsx server.ts",
    "start:dev": "tsx watch server.ts",
    "db:dev": "prisma migrate dev",
    "build": "tsup src",
    "test": "vitest run --dir src/services/tests",
    "test:watch": "vitest --dir src/services/tests",
    "db:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "db:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "test:e2e:all": "npm run db:test:up && npm run db:dev && npm run test:e2e && npm run db:test:down",
    "test:e2e": "vitest --dir src/controllers/users/tests --ui --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.5.5",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.1.2",
    "globals": "^15.9.0",
    "prisma": "^6.7.0",
    "supertest": "^7.1.1",
    "testcontainers": "^10.26.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "zod": "^3.24.3"
  }
}
